{
  "name": "vaszev/commons-bundle",
  "description": "Service of common functions that may useful in symfony2",
  "type": "symfony-bundle",
  "homepage": "https://github.com/vaszev/commons",
  "authors": [
    {
      "name": "Roland Bodnar",
      "email": "vaszev@gmail.com"
    }
  ],
  "keywords": [
    "bundle common useful symfony filter"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": ">=2.6",
    "ext-gd": "*",
    "gregwar/image": "2.*"
  },
  "minimum-stability":"dev",
  "autoload": {
    "psr-0": {
      "Vaszev\\CommonsBundle": ""
    }
  },
  "target-dir": "Vaszev/CommonsBundle",
  "config": {
    "preferred-install": "dist"
  }

}